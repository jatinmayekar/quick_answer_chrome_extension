{
    "manifest_version":3,
    "name": "My First Extension",
    "description": "The first extension that I made.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action":  {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}